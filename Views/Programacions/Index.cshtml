@model IEnumerable<CrudThor1.Models.Programacion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FechaProgramacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Instructor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DiaLaboral)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clase)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaProgramacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Instructor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DiaLaboral)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Clase.ClaseId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdProgramacion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdProgramacion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdProgramacion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
